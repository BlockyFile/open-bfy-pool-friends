{{#if model.matured}}
<h4 style="color: #1a191e;">Matured Blocks</h4>
<div class="table-responsive">
<table class="table table-condensed table-striped">
  <thead>
    <tr style="color: #1a191e;">
      <th>Height</th>
      <th>Login</th>
      <th>Time Found</th>
      <th>Variance</th>
      <th>Reward</th>
      <th>Type</th>
      <th>Shares/Diff</th>
      <th>Worker ID</th>
    </tr>
  </thead>
  <tbody style="color: #1a191e;">
    {{#each model.matured as |block|}}
      {{partial "blocks/block"}}
    {{/each}}
  </tbody>
</table>
</div>
{{else}}
<h3>No matured blocks yet</h3>
{{/if}}
